<h2>Program: <br><%= @program.name %></h2>
<%= link_to "Add new circuit", new_program_circuit_path(@program) %>

<% @program.circuits.each do |circuit| %>
    <ul>
    <h3><% circuit.exercises.each do |exercise| %></h3>
       
            <li><%= link_to exercise.name, program_circuits_path(exercise) %></li>
            <% end %>
            </ul>
    <% end %>
